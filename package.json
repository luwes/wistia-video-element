{
  "name": "wistia-video-element",
  "version": "0.2.0",
  "description": "Custom element (web component) for the Wistia player.",
  "type": "module",
  "main": "wistia-video-element.js",
  "repository": "luwes/wistia-video-element",
  "author": "Wesley Luyten <me@wesleyluyten.com> (https://wesleyluyten.com)",
  "license": "MIT",
  "homepage": "https://github.com/luwes/wistia-video-element#readme",
  "bugs": {
    "url": "https://github.com/luwes/wistia-video-element/issues"
  },
  "scripts": {
    "test": "web-test-runner --config test/web-test-runner.config.js",
    "dev": "npx serve ."
  },
  "dependencies": {
    "super-media-element": "^0.4.0"
  },
  "devDependencies": {
    "@open-wc/testing": "^3.1.7",
    "@web/test-runner": "^0.15.0",
    "eslint": "^8.32.0",
    "prettier": "^2.8.3"
  },
  "prettier": {
    "tabWidth": 2,
    "singleQuote": true,
    "semi": true
  },
  "eslintConfig": {
    "globals": {
      "globalThis": "writable"
    },
    "env": {
      "browser": true,
      "es6": true,
      "mocha": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2022,
      "sourceType": "module"
    }
  },
  "keywords": [
    "wistia",
    "video",
    "player",
    "web component",
    "custom element"
